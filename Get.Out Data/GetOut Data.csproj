<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net7.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <AssemblyName>Get.Out Data</AssemblyName>
        <RootNamespace>Get.Out Data</RootNamespace>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Microsoft.AspNetCore.Routing" Version="2.2.2" />
      <PackageReference Include="NLog" Version="5.1.0" />
      <PackageReference Include="NLog.Config" Version="4.7.15" />
      <PackageReference Include="NLog.Extensions.Logging" Version="5.2.0" />
      <PackageReference Include="NLog.Telegram" Version="1.1.2" />
      <PackageReference Include="NLog.Web.AspNetCore" Version="5.2.0" />
      <PackageReference Include="Telegram.Bot" Version="19.0.0-preview.1" />
      <PackageReference Include="Telegram.Bot.Extensions.Polling" Version="2.0.0-alpha.1" />
    </ItemGroup>

    <ItemGroup>
      <Content Include="bin\Debug\net7.0\configure.json" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="configure.json" />
    </ItemGroup>

</Project>
